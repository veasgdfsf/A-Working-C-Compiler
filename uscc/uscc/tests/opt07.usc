int main()
{
	int i = 10;
	int array[3];
	int value;
	array[0] = 1;
	array[1] = 3;
	array[2] = 5;
	value = array[1];
	
	while (i > 0)
	{
		int result = value + 10;
		printf("%d\n", result);
		--i;
	}
	
	printf("%d\n", array[2]);
	return 0;
}
