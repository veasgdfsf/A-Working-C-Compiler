int main()
{
	// Math expressions
	printf("%d\n", 5);
	printf("%d\n", 5 * 5);
	printf("%d\n", 5 / 5);
	printf("%d\n", 100 - 50);
	
	// Comparsion expressions
	printf("%d\n", 5 == 10);
	printf("%d\n", 5 != 10);
	printf("%d\n", 5 > 10);
	printf("%d\n", 5 < 10);
	
	return 0;
}
